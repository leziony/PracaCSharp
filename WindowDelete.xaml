<Window x:Class="ToDoList3.WindowDelete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToDoList3"
        mc:Ignorable="d"
        Title="Usun Zadanie" Height="200" Width="200">
    <Grid>
        <TextBox x:Name="ID_input" HorizontalAlignment="Center" Margin="0,53,0,0" TextWrapping="Wrap" PreviewTextInput="ID_input_PreviewTextInput" VerticalAlignment="Top" Width="120"/>
        <Button Content="Usuń" HorizontalAlignment="Center" VerticalAlignment="Top" Click="Button_Click" RenderTransformOrigin="0.505,0.697" Margin="0,115,0,0"/>
        <Label Content="Podaj ID Zadania do usunięcia" HorizontalAlignment="Left" Margin="18,9,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
